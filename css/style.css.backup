/* Base styles */
:root {
  --container-width: 73.5rem;
  --header-height: 5rem;
  --grid-gap: 0.25rem;
  --primary-color: #ffffff;
  --secondary-color: #9B9B9B;
  --background-color: #000000;
}

/* Reset and base styles */
* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

body {
  font-family: 'Futura PT', Arial, sans-serif;
  background-color: var(--background-color);
  color: var(--primary-color);
  line-height: 1.5;
}

.wrapper {
  min-height: 100vh;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}

/* Header styles */
.header {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 100;
  background-color: rgba(0, 0, 0, 0.95);
  backdrop-filter: blur(5px);
}

.header__container {
  max-width: var(--container-width);
  margin: 0 auto;
  padding: 0 0.938rem;
  height: var(--header-height);
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.header__logo {
  font-size: 1.125rem;
  font-weight: 700;
  color: var(--primary-color);
  text-decoration: none;
  z-index: 5;
}

.header__navigation {
  display: flex;
  align-items: center;
  gap: 2rem;
}

/* Menu styles */
.menu__body {
  display: flex;
  align-items: center;
}

.menu__list {
  display: flex;
  align-items: center;
  gap: 0.906rem;
  list-style: none;
}

.menu__link {
  color: var(--primary-color);
  text-decoration: none;
  font-size: 1rem;
  transition: color 0.3s ease;
}

.menu__link:hover {
  color: #006CD8;
}

.menu__link_active {
  color: #006CD8;
}

/* Actions header styles */
.actions-header {
  display: flex;
  align-items: center;
  gap: 1.25rem;
}

.social-icons {
  display: flex;
  align-items: center;
  gap: 0.625rem;
}

.theme-switcher {
  cursor: pointer;
}

/* Portfolio styles */
.portfolio {
  padding-top: calc(var(--header-height) + 2rem);
}

.portfolio__container {
  max-width: var(--container-width);
  margin: 0 auto;
  padding: 0 0.938rem;
}

.portfolio__grid {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: var(--grid-gap);
  padding: 2rem 0;
}

.portfolio__item {
  position: relative;
  aspect-ratio: 16/9;
  overflow: hidden;
}

.portfolio__item a {
  display: block;
  width: 100%;
  height: 100%;
  text-decoration: none;
}

.portfolio__item img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: transform 0.3s ease;
}

.portfolio__overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  display: flex;
  align-items: center;
  justify-content: center;
  opacity: 0;
  transition: opacity 0.3s ease;
}

.portfolio__title {
  color: var(--primary-color);
  font-size: 1.125rem;
  text-align: center;
  padding: 0 1rem;
  transform: translateY(20px);
  transition: transform 0.3s ease;
}

.portfolio__item:hover img {
  transform: scale(1.05);
}

.portfolio__item:hover .portfolio__overlay {
  opacity: 1;
}

.portfolio__item:hover .portfolio__title {
  transform: translateY(0);
}

/* Back to top button */
.back-to-top {
  position: fixed;
  bottom: 30px;
  right: 30px;
  width: 60px;
  height: 60px;
  background-color: #3B3B3B;
  border-radius: 60px;
  display: flex;
  align-items: center;
  justify-content: center;
  opacity: 0;
  transition: opacity 0.3s ease;
  cursor: pointer;
  z-index: 999;
}

.back-to-top svg {
  width: 18px;
  height: 18px;
}

.back-to-top path {
  stroke: white;
}

/* Footer styles */
.footer {
  margin-top: auto;
  padding: 1.5rem 0;
  width: 100%;
}

.footer__container {
  max-width: var(--container-width);
  margin: 0 auto;
  padding: 0 0.938rem;
  text-align: center;
  display: flex;
  justify-content: center;
}

.footer__copyright {
  color: var(--secondary-color);
  font-size: 0.875rem;
  text-align: center;
}

/* Menu icon for mobile */
.icon-menu {
  display: none;
  background: none;
  border: none;
  cursor: pointer;
  padding: 0.5rem;
  z-index: 5;
}

.icon-menu span,
.icon-menu::before,
.icon-menu::after {
  content: "";
  display: block;
  width: 1.5rem;
  height: 2px;
  background-color: var(--primary-color);
  margin: 4px 0;
  transition: all 0.3s ease;
}

/* =============== ABOUT PAGE (NEW DESIGN) =============== */
.about-page {
  color: #fff;
  overflow-x: hidden;
  padding-top: var(--header-height);
}

/* Main structure */
.about-main {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}

/* Cover Section */
.about-cover {
  position: relative;
  width: 100%;
  max-width: 920px;
  margin: 0 auto;
  height: 541px;
  overflow: hidden;
}

.about-cover__bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-image: url('../img/about/cover-bg.jpeg');
  background-size: cover;
  background-position: center;
  z-index: 1;
  border-radius: 0 0 12px 12px;
}

.about-cover__bg::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: linear-gradient(to bottom, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 1) 100%);
  z-index: 2;
}

/* Avatar */
.about-avatar {
  position: relative;
  width: 256.98px;
  height: 256.98px;
  border-radius: 128.49px;
  overflow: hidden;
  z-index: 3;
  border: 0px solid rgba(255, 255, 255, 0.1);
