@import url("reset.css");

@import url("https://fonts.googleapis.com/css?family=Inter:regular,500,600,700&display=swap");
@import url('https://fonts.googleapis.com/css2?family=Futura+PT:wght@400;500;700&display=swap');

/* ************CSS VARIABLES************ */
:root {
  /* Base colors */
  --color-primary: #006CD8;
  --color-secondary: #303a4d;
  --color-background: #000000;
  --color-text: #ffffff;
  --color-text-muted: #a0a0a0;
  --color-border: #333333;
  
  /* Font settings */
  --font-primary: "Futura PT", "Inter", sans-serif;
  --font-secondary: "Inter", sans-serif;
  --font-size-base: 1rem;
  --font-size-small: 0.875rem;
  --font-size-large: 1.125rem;
  --font-size-xl: 1.5rem;
  --font-size-xxl: 2rem;
  --font-size-title: 3.5rem;
  
  /* Spacing */
  --space-xs: 0.625rem;
  --space-sm: 0.938rem;
  --space-md: 1.25rem;
  --space-lg: 1.5rem;
  --space-xl: 2.5rem;
  --space-xxl: 3.75rem;
  
  /* Layout */
  --container-width: 72.624rem;
  --container-padding: 0.938rem;
  --header-height: 5rem;
  
  /* Transitions */
  --transition-fast: 0.3s;
  --transition-slow: 0.5s;
}

/* Light theme variables */
body.light-theme {
  --color-background: #ffffff;
  --color-text: #FFFFFF;
  --color-text-muted: #9B9B9B;
  --color-border: #e5e5e5;
  --color-secondary: #006CD8;
}

/* ************COMMON************ */

.logo {
  font-weight: 700;
  font-size: var(--font-size-large);
  font-family: var(--font-secondary);
  color: var(--color-text);
}

body {
  font-family: var(--font-primary);
  font-size: var(--font-size-base);
  color: var(--color-text);
  background-color: var(--color-background);
  transition: background-color var(--transition-fast), color var(--transition-fast);
  /* Disable text selection to prevent copying */
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}

/* ************MEDIA PROTECTION STYLES************ */
/* Apply to all images and videos to prevent download and unauthorized use */
.protected-media {
  /* Prevent context menu */
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  /* Prevent dragging */
  -webkit-user-drag: none;
  -khtml-user-drag: none;
  -moz-user-drag: none;
  -o-user-drag: none;
  /* Prevent saving through pointer events, but allow interaction */
  pointer-events: auto;
  /* Add subtle watermark-like effect */
  position: relative;
}

/* Watermark overlay for images */
.portfolio__item img.protected-media::after,
.project-detail__image img.protected-media::after {
  content: "© RAN 2025";
  position: absolute;
  bottom: 10px;
  right: 10px;
  font-size: 12px;
  color: rgba(255, 255, 255, 0.7);
  padding: 2px 5px;
  border-radius: 3px;
  background-color: rgba(0, 0, 0, 0.3);
  pointer-events: none;
  z-index: 10;
}

/* Video protection overlay */
.video-protection-overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: transparent;
  cursor: pointer;
  z-index: 5;
  display: flex;
  justify-content: center;
  align-items: center;
}

/* Style for protected videos */
.project-detail__video {
  width: 100%;
  margin: 0 auto 1.25rem;
}

.project-detail__video video {
  width: 100%;
  object-fit: cover;
  display: block;
}

/* Force videos to autoplay and hide controls */
video[autoplay] {
  -webkit-appearance: none;
  appearance: none;
}

video::-webkit-media-controls {
  display: none !important;
}

video::-webkit-media-controls-enclosure {
  display: none !important;
}

/* Video grid layout for side-by-side videos */
.video-grid {
  display: flex;
  flex-wrap: nowrap;
  gap: 1rem;
  margin-bottom: 1.25rem;
}

.video-grid .project-detail__video {
  flex: 1;
  min-width: 30%;
  margin-bottom: 0;
}

[class*="__container"] {
  max-width: var(--container-width);
  margin: 0 auto;
  padding: 0 var(--space-sm);
}

.button {
  font-weight: 600;
  display: inline-block;
  padding: 0.75rem 1.5rem;
  border-radius: 30px;
  transition: all var(--transition-fast);
  color: #fff;
  background-color: var(--color-primary);
}

.button:hover {
  opacity: 0.9;
}

.title {
  font-size: var(--font-size-title);
  font-weight: 600;
  color: var(--color-text);
}

.wrapper {
  min-height: 100%;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -ms-flex-direction: column;
  flex-direction: column;
  overflow: hidden;
}

/* ************HEADER************ */

.header {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background: var(--color-background);
  z-index: 1000;
  transition: background-color var(--transition-fast);
}

.header__container {
  display: flex;
  align-items: center;
  justify-content: space-between;
  min-height: var(--header-height);
  padding: 25px 57px;
  max-width: 100%;
}

.header__logo {
  position: relative;
  z-index: 2;
  font-family: var(--font-secondary);
  font-weight: 700;
  font-size: 18.125px;
  line-height: 1.65em;
}

.header__navigation {
  display: flex;
  align-items: center;
}

.header__nav-list {
  display: flex;
  gap: 14.5px;
}

.header__nav-item {
  list-style: none;
}

.header__nav-link {
  font-family: var(--font-primary);
  font-weight: 400;
  font-size: 16px;
  line-height: 1.5em;
  color: var(--color-text);
  transition: color var(--transition-fast);
  display: inline-block;
  padding: 5px 0;
}

.header__nav-link:hover {
  color: var(--color-primary);
}

.header__nav-link_active {
  color: var(--color-primary);
}

.icon-menu {
  display: none;
  position: relative;
  width: 30px;
  height: 18px;
  cursor: pointer;
  z-index: 5;
  background: transparent;
  border: none;
}

.icon-menu span,
.icon-menu::before,
.icon-menu::after {
  content: "";
  background-color: var(--color-text);
  height: 2px;
  width: 100%;
  position: absolute;
  left: 0;
  transition: all var(--transition-fast);
}

.icon-menu::before {
  top: 0;
}

.icon-menu::after {
  bottom: 0;
}

.icon-menu span {
  top: 50%;
  transform: translateY(-50%);
}

.icon-menu.active span {
  transform: scale(0);
}

.icon-menu.active::before {
  top: 50%;
  transform: rotate(-45deg);
}

.icon-menu.active::after {
  bottom: auto;
  top: 50%;
  transform: rotate(45deg);
}

/* Media queries for header */
@media (max-width: 768px) {
  .header__container {
    padding: 15px 20px;
  }
  
  .icon-menu {
    display: none; /* Hide the hamburger menu */
  }
  
  .header__navigation {
    position: relative;
    left: 0;
    width: auto;
    height: auto;
    background-color: transparent;
    padding: 0;
    overflow: visible;
    transition: none;
    z-index: 2;
  }
  
  .menu-open .header__navigation {
    left: 0;
  }
  
  .header__nav-list {
    flex-direction: row;
    gap: 15px;
    align-items: center;
  }
  
  .header__nav-link {
    font-size: 16px;
    font-weight: 400;
    padding: 5px;
  }
  
  .portfolio__title {
    font-size: 24px;
  }
  
  .project-detail__title {
    font-size: var(--font-size-xxl);
  }
  
  .project-detail__description {
    font-size: var(--font-size-base);
  }
  
  .back-to-top {
    bottom: 20px;
    right: 20px;
    width: 50px;
    height: 50px;
  }
  
  .back-to-top svg {
    width: 16px;
    height: 16px;
  }
}

@media (max-width: 576px) {
  .header__container {
    padding: 10px 15px;
  }
  
  .header__logo {
    font-size: 16px;
  }
  
  .header__nav-list {
    gap: 10px;
  }
  
  .header__nav-link {
    font-size: 14px;
    padding: 3px;
  }
  
  .back-to-top {
    bottom: 15px;
    right: 15px;
    width: 40px;
    height: 40px;
  }
  
  .back-to-top svg {
    width: 14px;
    height: 14px;
  }
}

@media (max-width: 400px) {
  .header__container {
    flex-wrap: wrap;
    justify-content: center;
    gap: 5px;
  }
  
  .header__logo {
    margin-bottom: 5px;
  }
  
  .back-to-top {
    bottom: 10px;
    right: 10px;
    width: 35px;
    height: 35px;
  }
  
  .back-to-top svg {
    width: 12px;
    height: 12px;
  }
}

